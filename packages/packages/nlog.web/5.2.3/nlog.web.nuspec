    <dependencies>
      <group targetFramework=".NETFramework3.5">
        <dependency id="NLog" version="5.1.3" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.6">
        <dependency id="NLog" version="5.1.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web.Abstractions" targetFramework=".NETFramework3.5, .NETFramework4.6" />
      <frameworkAssembly assemblyName="System.Web" targetFramework=".NETFramework3.5, .NETFramework4.6" />
      <frameworkAssembly assemblyName="System.Web.Routing" targetFramework=".NETFramework3.5, .NETFramework4.6" />
    </frameworkAssemblies>
